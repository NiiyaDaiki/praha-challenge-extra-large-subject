datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

generator client {
  provider        = "prisma-client-js"
}

model Participant {
  id         String       @id
  name       String
  email      String       @unique
  status     Status       @default(ACTIVE)
}

enum Status {
  ACTIVE
  INACTIVE
  LEFT
}

model SomeData {
  id        String    @id
  required Boolean
  number   Int     @db.SmallInt
}
